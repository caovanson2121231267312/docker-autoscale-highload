# Application
APP_NAME=LaravelProduction
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://example.com

# Database
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=laravel
DB_PASSWORD=

# Redis
REDIS_HOST=redis
REDIS_PASSWORD=
REDIS_PORT=6379
REDIS_CLIENT=phpredis

# Cache
CACHE_DRIVER=redis
SESSION_DRIVER=redis
QUEUE_CONNECTION=redis

# Nginx
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# PHP-FPM
PHP_FPM_PM=dynamic
PHP_FPM_MAX_CHILDREN=100
PHP_FPM_START_SERVERS=20
PHP_FPM_MIN_SPARE_SERVERS=10
PHP_FPM_MAX_SPARE_SERVERS=30
PHP_FPM_MAX_REQUESTS=1000

# MySQL
MYSQL_PORT=3306

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=error